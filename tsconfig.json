{
  "include": [
    "scripts/**/*"
  ],
  "exclude": [
    "node_modules",
    "scripts/service-worker.ts"
  ],
  "compilerOptions": {
    "moduleResolution": "Node",
    "target": "es2022",
    "module": "es2022",
    "lib": [
      "dom",
      "es5",
      "scripthost",
      "es2015.promise",
      "ESNext"
    ]
  },
  "tsdocsOptions": {
    "name": "Fappen",
    "includeVersion": "Beta 1",
    "entryPoints": [
      "scripts/index"
    ],
    "exclude": "scripts/service-worker.ts",
    "out": "build/docs",
    "customPages": "docs"
  }
}